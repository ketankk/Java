package kk.java.calc;

import java.util.Scanner;

import kk.java.add.Add;
import kk.java.subdiv.SubDiv;

public class Calculator extends SubDiv{

	static Add ad=new Add();
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.print("Enter Two no.");
		Scanner sc=new Scanner(System.in);
		int a= sc.nextInt();
		int b= sc.nextInt();
		char ch ='+';//sc.next().charAt(0);
		int c=0;
		
		switch(ch){
		case '+' : c=ad.Add(a,b);
			break;
		case '-': c=a-b;
			break;
			case '*': c=a*b;
				break;
			case '/': c=a/b;
				break;
				default:
					System.out.print("Enter Valid operator");
		
		}					System.out.print("Enter Valid operator"+a+b);

	}

}
